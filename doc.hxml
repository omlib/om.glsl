--no-output
-lib om.glsl
om.glsl.Tokenizer
--macro keep('om.glsl.Tokenizer')
--macro include('om.glsl.Tokenizer')
-D doc
-D doc-gen
--each

--next
-cpp x
-xml doc/cpp.xml

--next
-cs x
-xml doc/cs.xml

--next
-hl x
-xml doc/hl.xml

--next
-java x
-xml doc/java.xml

--next
-js x
-xml doc/js.xml

--next
-lua x
-xml doc/lua.xml

--next
-neko x
-xml doc/neko.xml

--next
-js x
-lib hxnodejs
-xml doc/nodejs.xml

--next
-php x
-xml doc/php.xml

--next
-python x
-xml doc/python.xml
